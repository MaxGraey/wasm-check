{
  "name": "wasm-check",
  "version": "0.2.0",
  "author": "MaxGraey @ 2019",
  "license": "MIT",
  "description": "TypeScript / JavaScript library for detect WebAssembly features in node.js & browser",
  "keywords": [
    "wasm",
    "webassembly",
    "feature",
    "validate",
    "check",
    "support",
    "test",
    "detect"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "bugs": "https://github.com/MaxGraey/wasm-check/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxGraey/wasm-check.git"
  },
  "engineStrict" : true,
  "engines": {
    "node": ">= 8"
  },
  "devDependencies": {
    "@types/webassembly-js-api": "^0.0.3",
    "@types/webassembly-web-api": "^0.0.1",
    "typescript": "^3.4.2"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "yarn build"
  }
